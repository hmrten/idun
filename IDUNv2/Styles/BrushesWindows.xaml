<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:IDUNv2.Styles">

    <SolidColorBrush x:Key="MainMenuBrush" Color="Black"></SolidColorBrush>
    <SolidColorBrush x:Key="SubMenuBrush" Color="White"></SolidColorBrush>
    <SolidColorBrush x:Key="ButtonBrush" Color="Yellow"></SolidColorBrush>
    <SolidColorBrush x:Key="StandardBackground" Color="Orange"></SolidColorBrush>
    <SolidColorBrush x:Key="StandardForeground" Color="Blue"></SolidColorBrush>
    <SolidColorBrush x:Key="StandardBorder" Color="Pink"></SolidColorBrush>
    <SolidColorBrush x:Key="TextHighLight" Color="Red"></SolidColorBrush>
    <SolidColorBrush x:Key="NotificationBackground" Color="DarkGoldenrod"></SolidColorBrush>
    <SolidColorBrush x:Key="NotificationListBackground" Color="DarkGreen"></SolidColorBrush>
    <SolidColorBrush x:Key="ExtendedNotificationBackground" Color="DarkGray"></SolidColorBrush>

</ResourceDictionary>
